{
  "name": "tomato",
  "version": "0.0.1",
  "description": "Example of a forum made using express and backbone",
  "main": "server/server.js",
  "repository": "git@github.com:Stram/tomato-forum.git",
  "scripts": {
    "start": "DATABASE_URL=localhost:27017/express nodemon server/server.js --watch server --exec babel-node --presets es2015,stage-2",
    "startServer": "webpack --config webpack.config.server.js --progress --colors --watch",
    "webpackDev": "webpack --progress --colors --watch",
    "build": "webpack --optimize-minimize --optimize-dedupe",
    "docs": "apidoc -i server/",
    "test": "NODE_ENV=testing istanbul cover node_modules/.bin/_mocha -- --require babel-register -R spec 'server/**/test.js'"
  },
  "author": "Stjepan Petrusa",
  "license": "ISC",
  "apidoc": {
    "title": "Tomato api doc",
    "version": "0.1.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-fileupload": "0.0.5",
    "express-session": "^1.13.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "markdown-loader": "^0.1.7",
    "mongoose": "^4.5.1",
    "mongoose-deep-populate": "^3.0.0",
    "morgan": "^1.7.0",
    "nodemailer": "^2.4.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "rand-token": "^0.2.1",
    "validator": "^5.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.11.6",
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.0.0-pre.5",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "css-loader": "^0.23.1",
    "dropzone": "^4.3.0",
    "eslint": "^2.13.1",
    "fs": "0.0.2",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "istanbul": "^1.0.0-alpha.2",
    "jquery": "^3.1.0",
    "mocha": "^3.0.1",
    "mockgoose": "^6.0.6",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "sass-loader": "^3.2.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "supertest-session": "^2.0.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1"
  }
}
