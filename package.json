{
  "name": "tomato",
  "version": "0.0.1",
  "description": "Example of a forum made using express with typescript, and react",
  "main": "server/index.js",
  "repository": "git@github.com:Stram/tomato-forum.git",
  "scripts": {
    "start": "DATABASE_URL=localhost:27017/express nodemon server/index.js --watch server --exec babel-node --presets es2015,stage-2",
    "server": "node build.js",
    "webpackDev": "webpack --progress --colors --watch -d",
    "build": "node_modules/.bin/webpack -p --optimize-dedupe --config webpack-prod.config.js && node_modules/.bin/webpack --config webpack.config.server.js",
    "react": "webpack --progress --colors --watch -d --config webpack-react.config.js",
    "docs": "apidoc -i server/",
    "test": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- --require babel-register -R spec 'server/**/test.js'",
    "server:coverage": "NODE_ENV=test node_modules/.bin/nyc npm run server:test",
    "server:test": "NODE_ENV=test NODE_PATH=server TS_NODE_PROJECT=server node_modules/.bin/_mocha --opts server/tests/mocha.opts",
    "server:dev": "NODE_ENV=development NODE_PATH=server TS_NODE_PROJECT=server ts-node server/index.ts"
  },
  "author": "Stjepan Petrusa",
  "license": "ISC",
  "apidoc": {
    "title": "Tomato api doc",
    "version": "0.1.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "dropzone": "^4.3.0",
    "express": "^4.14.0",
    "express-fileupload": "0.0.5",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "handlebars": "^4.0.5",
    "handlebars-template-loader": "^0.7.0",
    "jquery": "^3.1.0",
    "lodash": "^4.13.1",
    "markdown-loader": "^0.1.7",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.5",
    "mongoose": "^4.5.1",
    "mongoose-deep-populate": "^3.0.0",
    "mongoose-paginate": "^5.0.0",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
    "nodemailer": "^2.4.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "rand-token": "^0.2.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "sass-loader": "^3.2.1",
    "validator": "^6.2.1",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/connect-mongo": "0.0.32",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/lodash": "^4.14.51",
    "@types/mocha": "^2.2.38",
    "@types/mongoose": "^4.7.3",
    "@types/mongoose-deep-populate": "^2.0.29",
    "@types/mongoose-paginate": "^5.0.4",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.4",
    "@types/passport": "^0.3.2",
    "@types/passport-local": "^1.0.29",
    "@types/pg": "^6.1.35",
    "@types/supertest": "^2.0.0",
    "@types/validator": "^5.7.35",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-eslint": "^6.1.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^6.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "mobx-react-devtools": "^4.2.6",
    "mocha": "^3.0.1",
    "mockgoose": "^6.0.6",
    "nodemon": "^1.9.2",
    "supertest": "^2.0.0",
    "supertest-session": "^2.0.1",
    "ts-node": "^2.0.0",
    "typescript": "^2.1.5",
    "webpack-livereload-plugin": "^0.8.2"
  }
}
